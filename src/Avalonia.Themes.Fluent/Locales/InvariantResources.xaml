<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    x:ClassModifier="internal">
  <!-- DatePicker -->
  <x:String x:Key="LocaleDatePickerDayText">day</x:String>
  <x:String x:Key="LocaleDatePickerMonthText">month</x:String>
  <x:String x:Key="LocaleDatePickerYearText">year</x:String>
  <!-- TimePicker -->
  <x:String x:Key="LocaleTimePickerHourText">hour</x:String>
  <x:String x:Key="LocaleTimePickerMinuteText">minute</x:String>
  <!-- TextBox/SelectableTextBox flyout -->
  <x:String x:Key="LocaleTextFlyoutCutText">Cut</x:String>
  <x:String x:Key="LocaleTextFlyoutCopyText">Copy</x:String>
  <x:String x:Key="LocaleTextFlyoutPasteText">Paste</x:String>
  <!-- ManagedFileChooser -->
  <x:String x:Key="LocaleManagedFileChooserFileNameWatermark">File name</x:String>
  <x:String x:Key="LocaleManagedFileChooserShowHiddenFilesText">Show hidden files</x:String>
  <x:String x:Key="LocaleManagedFileChooserOkText">OK</x:String>
  <x:String x:Key="LocaleManagedFileChooserCancelText">Cancel</x:String>
  <x:String x:Key="LocaleManagedFileChooserNameColumn">Name</x:String>
  <x:String x:Key="LocaleManagedFileChooserDateModifiedColumn">Date Modified</x:String>
  <x:String x:Key="LocaleManagedFileChooserTypeColumn">Type</x:String>
  <x:String x:Key="LocaleManagedFileChooserSizeColumn">Size</x:String>
  <x:String x:Key="LocaleManagedFileChooserOverwritePromptFileAlreadyExistsText">{0} already exists. Do you want to replace it?</x:String>
  <x:String x:Key="LocaleManagedFileChooserOverwritePromptConfirmText">Yes</x:String>
  <x:String x:Key="LocaleManagedFileChooserOverwritePromptCancelText">No</x:String>
</ResourceDictionary>

