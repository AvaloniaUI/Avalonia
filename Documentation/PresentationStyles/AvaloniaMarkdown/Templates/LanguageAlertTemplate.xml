<table>
	<tr>
		<th>
			<img src="{@IconPath}AlertLanguage.png">
				<includeAttribute name="alt" item="{@AlertAltTextItemId}" />
			</img>&#160;&#160;<include item="{@AlertTitleItemId}"/>
