<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <SharpDXPackageVersion>4.0.1</SharpDXPackageVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="SharpDX" />
    <PackageReference Include="SharpDX.Direct2D1" />
    <PackageReference Include="SharpDX.Direct3D11" />
    <PackageReference Include="SharpDX.DXGI" />
    <PackageReference Include="SharpDX.Direct3D9" Condition="'$(UseDirect3D9)' == 'true'" />
    <PackageReference Include="SharpDX.D3DCompiler" Condition="'$(UseD3DCompiler)' == 'true'" />
    <PackageReference Include="SharpDX.Mathematics" Condition="'$(UseSharpDXMathematics)' == 'true'" />
  </ItemGroup>
</Project>
